/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
/* 覆盖 TDesign 图标字体为本地文件，避免 CDN 加载失败 */
@font-face {
  font-family: 't-icon';
  src: url('/assets/fonts/t.woff') format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}